    "mem=[]\n",
    "def fib(n):\n",
    "    if mem[n] != 0:\n",
    "        return mem[n]\n",
    "    if n <= 2:\n",
    "        return 1\n",
    "    res = fib(n-1) + fib(n-2)\n",
    "    mem[n] = res\n",
    "fib(int(input()))"
